{
	"version": 2,
	"installCommand": "npm install --workspaces --include-workspace-root",
	"buildCommand": "npm run build",
	"outputDirectory": "build",
	"builds": [
		{
			"src": "build/index.js",
			"use": "@vercel/node",
			"config": { "includeFiles": ["build/**"] }
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "build/index.js"
		}
	]
}
