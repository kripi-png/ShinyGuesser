{
	"version": 2,
	"installCommand": "npm install --workspaces --include-workspace-root",
	"buildCommand": "npm run build",
	"outputDirectory": "build",
	"builds": [
      { "src": "build/app.js", "use": "@vercel/node" }
   ],
   "routes": [
      { "src": "/(.*)", "dest": "build/index.js" }
   ]

}
