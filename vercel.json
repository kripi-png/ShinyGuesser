{
	"version": 2,
	"installCommand": "npm install --workspaces --include-workspace-root",
	"buildCommand": "npm run build",
	"outputDirectory": "build",
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": { "distDir": "build" }
		},
		{
			"src": "index.js",
			"use": "@vercel/node"
		}
	]
}
