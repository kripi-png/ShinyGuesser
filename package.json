{
	"name": "shinyguesser",
	"version": "1.0.0",
	"description": "",
	"main": "server/index.ts",
	"scripts": {
		"start:server": "nodemon server/index.ts",
		"start:client": "cd vue && npm run dev",
		"dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
		"build": "rimraf dist server/views && tsc && npm run client-build && npm run copy-files && rimraf server/views",
		"add-build": "git add dist",
		"client-install": "cd vue && npm i",
		"client-build": "cd vue && npm i && npx vite build --emptyOutDir --outDir ../server/views",
		"copy-files": "copyfiles -u 1 server/views/** dist/",
		"format": "prettier --write ."
	},
	"author": "kripi-png",
	"license": "ISC",
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.11.5",
		"@vercel/kv": "^1.0.1",
		"axios": "^1.6.5",
		"axios-cache-interceptor": "^1.5.1",
		"concurrently": "^8.2.2",
		"copyfiles": "^2.4.1",
		"cors": "^2.8.5",
		"dotenv": "^16.3.2",
		"express": "^4.18.2",
		"nodemon": "^3.0.3",
		"pokemon": "^2.3.6",
		"pokenode-ts": "^1.20.0",
		"prettier": "^3.2.4",
		"prettier-plugin-organize-imports": "^3.2.4",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	}
}
