{
	"compilerOptions": {
		"outDir": "./dist",
		"target": "ESNext",
		"module": "ESNext",
		"resolveJsonModule": true,
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"noImplicitAny": true
	},
	"include": ["api"],
	"exclude": ["svelte"]
}
